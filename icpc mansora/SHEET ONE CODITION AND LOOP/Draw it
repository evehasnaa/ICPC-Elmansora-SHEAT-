#include <iostream>

using namespace std;
void sympole(char s,int n)
{
    for (int i=0; i<n; i++)
    {
            cout <<s;
        cout <<endl;
    }

}
int main()
{
   
    char s;
    cin>>s;
    int t;
    cin>>t;

    for (int i=0; i<t; i++)
    {
        int n;
        cin>>n;
        for (int i=0; i<n; i++)
        {
                cout <<s;
        }
         cout <<endl;
    }

    return 0;
}
